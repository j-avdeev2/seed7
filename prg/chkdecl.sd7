      write("In recursive: loc_num = ");
      write(loc_num);
      write(" but should be ");
      writeln(20);
      incr(errCnt);
    end if;
    loc_num := number;
    if number > 0 then
      recursive(pred(number));
    end if;
    if loc_num <> number then
      write("In recursive: loc_num = ");
      write(loc_num);
      write(" but should be ");
      writeln(number);
      incr(errCnt);
    end if;
  end func;


